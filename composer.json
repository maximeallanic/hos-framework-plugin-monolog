{
    "name": "hos/plugin-twig-framework",
    "description": "Twig Plugin Framework for Fast Developpement for PHP",
    "keywords": [ "daehl", "hos", "framework", "twig" ],
    "homepage": "https://github.com/daehl/hos-framework",
    "type": "library",
    "license": "CC0 1.0",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Maxime Allanic",
            "email": "maxime.allanic@daehl.com"
        }
    ],
    "autoload": {
        "psr-4":{
            "Hos\\Plugin-Monolog": "src/"
        }
    },
     "require": {
        "monolog/monolog": "^1.18"
    },
    "config": {
      "dev": [
        {
          "file": "app/log/dev"
        }
      ],
      "prod": false
    },
    "events": {
      "log": {
        "info": "Hos\\Plugin-Monolog\\Info",
        "warning": "Hos\\Plugin-Monolog\\Warning",
        "error": "Hos\\Plugin-Monolog\\Error"
      }
    }

}
